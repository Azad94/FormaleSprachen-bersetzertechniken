package recursiveDescent;

public class Token {
	public int type;
	public String text;

	public Token(int type, String text) {
		this.type = type;
		this.text = text;
	}

	public String toString() {
		String tname = RDLexer.tokenNames[type];
		return "<'" + text + "'," + tname + ">";
	}
	
	public String getText(){
		return this.text;
	}
	public int getType(){
		return this.type;
	}
}
